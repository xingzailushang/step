//将三维对象数组转成二维数组
function objToArr(obj){
  var arr2=[];
  //将返回的三维对象数组转为二维对象数组
  for(var i=0;i<obj.length;i++){
    arr2[i]={};
    for(var key in obj[i]){
      if(typeof(eval("obj[i]."+key))=="object"){
        for(var key1 in eval("obj[i]."+key)){
          eval("arr2[i]."+key1+"=obj[i]."+key+"."+key1);
        }
      }else{
        eval("arr2[i]."+key+"=obj[i]."+key);
      }
    }
  }
  return arr2;
}
